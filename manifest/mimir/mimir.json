{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "MimirS3Permissions",
        "Effect": "Allow",
        "Action": [
      "s3:PutObject",
      "s3:GetObject",
      "s3:ListBucket",
      "s3:DeleteObject",
      "s3:GetAccountPublicAccessBlock",
      "s3:GetBucketAcl",
      "s3:GetBucketLocation",
      "s3:GetBucketPolicyStatus",
      "s3:GetBucketPublicAccessBlock",
      "s3:ListAllMyBuckets",
      "s3:GetLifecycleConfiguration",
      "s3:PutLifecycleConfiguration",
      "s3:*Object"
        ],
        "Resource": [
          "arn:aws:s3:::mimir-data-bucket",
          "arn:aws:s3:::mimir-data-bucket/*"
        ]
      }
    ]
  }


  